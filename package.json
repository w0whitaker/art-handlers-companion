  },
  "homepage": "https://github.com/w0whitaker/art-handlers-companion#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-task-lists": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.52.3"
  }
